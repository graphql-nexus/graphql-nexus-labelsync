{
  "name": "labelsync",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "make": "ts-node scripts/make",
    "sync": "git submodule foreach git pull origin master"
  },
  "dependencies": {
    "husky": "^4.2.3",
    "label-sync": "3.1.15"
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn -s make && git add labelsync.yml"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  }
}
